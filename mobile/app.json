{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "scheme": "nlwspacetime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/splash-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker", {
          "photosPermission": "The app accesses your photos/videos to let you share your spacetime."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
